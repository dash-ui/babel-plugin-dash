{
  "name": "babel-plugin-dash",
  "version": "0.1.1",
  "homepage": "https://github.com/dash-ui/babel-plugin-dash#readme",
  "repository": "github:dash-ui/babel-plugin-dash",
  "bugs": "https://github.com/dash-ui/babel-plugin-dash/issues",
  "license": "MIT",
  "description": "A babel plugin for dash-ui",
  "keywords": [
    "babel plugin dash"
  ],
  "main": "dist/main/index.js",
  "source": "src/index.js",
  "files": [
    "/dist",
    "/src"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/main/index.js",
      "source": "./src/index.js",
      "default": "./dist/main/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build-esm && npm run build-main",
    "build-esm": "npm run compile -- -d dist/esm --env-name esm --out-file-extension .mjs",
    "build-main": "npm run compile -- -d dist/main --env-name main --out-file-extension .js",
    "compile": "babel src -x .js --ignore \"**/*.test.js\" --delete-dir-on-start",
    "format": "prettier --write \"**/*.{js,md,yml,json,eslintrc,prettierrc}\"",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run format",
    "test": "jest",
    "validate": "npm run lint && npm run test -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --write"
    ],
    "**/*.{md,yml,json,eslintrc,prettierrc}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@lunde/babel-preset-es": "latest",
    "babel-check-duplicated-nodes": "^1.0.0",
    "babel-eslint": "latest",
    "babel-plugin-annotate-pure-calls": "latest",
    "eslint": "latest",
    "eslint-import-resolver-jest": "latest",
    "eslint-plugin-jest": "latest",
    "husky": "latest",
    "jest": "latest",
    "jest-in-case": "^1.0.2",
    "lint-staged": "latest",
    "prettier": "latest"
  },
  "dependencies": {
    "@babel/generator": "^7.9.6",
    "@babel/helper-module-imports": "^7.8.3",
    "@dash-ui/styles": "^0.4.4",
    "babel-plugin-macros": "^2.8.0",
    "convert-source-map": "^1.7.0",
    "source-map": "^0.7.3"
  },
  "peerDependencies": {}
}
